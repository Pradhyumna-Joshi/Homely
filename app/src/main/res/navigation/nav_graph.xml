<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginIntro">

    <fragment
        android:id="@+id/loginIntro"
        android:name="com.example.homely.LoginIntro"
        android:label="fragment_login_intro"
        tools:layout="@layout/fragment_login_intro" >
        <action
            android:id="@+id/action_loginIntro_to_logInFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/loginIntro"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginIntro_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.homely.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/loginIntro"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_loginFragment_to_forgetPassword"
            app:destination="@id/forgetPassword" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.homely.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_signInFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/loginIntro"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.homely.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_tenant_DashBoard"
            app:destination="@id/tenant_DashBoard"
            app:popUpTo="@id/loginFragment" />
        <action
            android:id="@+id/action_mainFragment_to_owner_form"
            app:destination="@id/ownerForm" />
        <action
            android:id="@+id/action_mainFragment_to_owner_Dashboard"
            app:destination="@id/owner_Dashboard" />
    </fragment>
    <fragment
        android:id="@+id/tenant_DashBoard"
        android:name="com.example.homely.Tenant_DashBoard"
        android:label="fragment_tenant__dash_board"
        tools:layout="@layout/fragment_tenant__dash_board" >
        <action
            android:id="@+id/action_tenant_DashBoard_to_itemStay"
            app:destination="@id/itemStay"
            app:popUpTo="@id/tenant_DashBoard" />
    </fragment>
    <fragment
        android:id="@+id/ownerForm"
        android:name="com.example.homely.Owner_Form"
        android:label="fragment_owner__form"
        tools:layout="@layout/fragment_owner_form">
        <action
            android:id="@+id/action_ownerForm_to_owner_Dashboard"
            app:destination="@id/owner_Dashboard"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/owner_Dashboard"
        android:name="com.example.homely.Owner_Dashboard"
        android:label="fragment_owner__dashboard"
        tools:layout="@layout/fragment_owner__dashboard" >
        <action
            android:id="@+id/action_owner_Dashboard_to_addStay"
            app:destination="@id/addStay" />
        <action
            android:id="@+id/action_owner_Dashboard_to_editStay"
            app:destination="@id/editStay" />
    </fragment>
    <fragment
        android:id="@+id/addStay"
        android:name="com.example.homely.AddStay"
        android:label="fragment_add_stay"
        tools:layout="@layout/fragment_add_stay" >
        <action
            android:id="@+id/action_addStay_to_facilites"
            app:destination="@id/facilites" />
    </fragment>
    <fragment
        android:id="@+id/editStay"
        android:name="com.example.homely.EditStay"
        android:label="fragment_edit_stay"
        tools:layout="@layout/fragment_edit_stay" >
        <action
            android:id="@+id/action_editStay_to_editFacilites"
            app:destination="@id/editFacilites" />
        <argument
            android:name="itemStay"
            app:argType="com.example.homely.Models.Facilities" />
    </fragment>
    <fragment
        android:id="@+id/itemStay"
        android:name="com.example.homely.ItemStay"
        android:label="fragment_item_stay"
        tools:layout="@layout/fragment_item_stay" />
    <fragment
        android:id="@+id/facilites"
        android:name="com.example.homely.Facilites"
        android:label="fragment_facilites"
        tools:layout="@layout/fragment_facilites" >
        <action
            android:id="@+id/action_facilites_to_owner_Dashboard"
            app:destination="@id/owner_Dashboard" />
    </fragment>
    <fragment
        android:id="@+id/editFacilites"
        android:name="com.example.homely.editFacilites"
        android:label="fragment_edit_facilites"
        tools:layout="@layout/fragment_edit_facilites" >
        <argument
            android:name="itemStay"
            app:argType="com.example.homely.Models.Facilities" />
        <action
            android:id="@+id/action_editFacilites_to_owner_Dashboard"
            app:destination="@id/owner_Dashboard" />
    </fragment>
    <fragment
        android:id="@+id/forgetPassword"
        android:name="com.example.homely.ForgetPassword"
        android:label="fragment_forget_password"
        tools:layout="@layout/fragment_forget_password" >
        <action
            android:id="@+id/action_forgetPassword_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
</navigation>